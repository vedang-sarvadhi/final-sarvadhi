{
  "name": "vite-min-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"bun run server\" \"vite\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "check": "bun x biome check .",
    "format": "bun x biome format . --write",
    "ci": "bun x biome check --apply-unsafe .",
    "server": "bun run server.js"
  },
  "dependencies": {
    "@mantine/core": "8.2.5",
    "@mantine/dates": "^8.2.5",
    "@mantine/hooks": "8.2.5",
    "@mantine/notifications": "^8.3.1",
    "@react-router/fs-routes": "^7.8.2",
    "@tabler/icons-react": "^3.34.1",
    "@tanstack/react-query": "^5.85.5",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "json-server": "^1.0.0-beta.3",
    "msw": "^2.11.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.62.0",
    "react-router": "7.8.2",
    "react-router-dom": "7.8.2",
    "recharts": "^3.1.2",
    "tailwindcss": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "concurrently": "^9.2.1",
    "postcss": "^8.5.5",
    "postcss-preset-mantine": "1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "typescript": "^5.8.3",
    "vite": "^7.1.7",
    "@types/bun": "latest"
  },
  "packageManager": "yarn@4.9.2",
  "module": "index.ts"
}